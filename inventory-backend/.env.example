# Copy this file to .env and fill in real values.

# Postgres connection string used by Prisma at runtime and for CLI commands
DATABASE_URL="postgresql://USER:PASSWORD@HOST:5432/DB_NAME?schema=public"

# Optional but recommended for production
NODE_ENV="development"

# JWKS URL used by Neon auth strategy (set to your Neon project JWKS endpoint)
NEON_JWKS_URL="https://<your-neon-project>.eu-central-1.aws.neon.tech/.well-known/jwks.json"

PORT=3000