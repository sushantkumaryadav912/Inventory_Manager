# Copy this file to .env and fill in real values.

# Postgres connection string used by Prisma at runtime and for CLI commands
DATABASE_URL="postgresql://USER:PASSWORD@HOST:5432/DB_NAME?schema=public"

# Optional but recommended for production
NODE_ENV="development"

# JWKS URL used by Neon auth strategy (set to your Neon project JWKS endpoint)
NEON_JWKS_URL="https://<your-neon-project>.eu-central-1.aws.neon.tech/.well-known/jwks.json"

# Port for the application
PORT=3000

# [OPTIONAL] Comma-separated list of allowed CORS origins for production security
# If not set or empty, backend will accept requests from all origins
# Set this once your frontend is deployed for better security
# Example: ALLOWED_ORIGINS="https://yourdomain.com,https://www.yourdomain.com"
# ALLOWED_ORIGINS=""